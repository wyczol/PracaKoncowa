<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="526.0" prefWidth="953.0" xmlns="http://javafx.com/javafx/8.0.161" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MazWindowController">
   <children>
      <BarChart fx:id="BarChart" layoutX="212.0" layoutY="22.0" prefHeight="458.0" prefWidth="630.0" title="Województwo Warmińsko - Mazurskie">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="x" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" side="LEFT" />
        </yAxis>
      </BarChart>
      <Button fx:id="dodajButton" layoutX="59.0" layoutY="98.0" mnemonicParsing="false" onMouseClicked="#SalesUpdateMaz" text="Dodaj" />
      <Label fx:id="lblCar" layoutX="14.0" layoutY="22.0" prefHeight="17.0" prefWidth="198.0" text="Dodaj nowe samochody:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="raportButton" layoutX="57.0" layoutY="142.0" mnemonicParsing="false" text="Raport" />
      <TextField fx:id="txtFieldCar" layoutX="14.0" layoutY="59.0" onMouseClicked="#SalesUpdateMaz" />
      <Button fx:id="Refresh" layoutX="57.0" layoutY="186.0" mnemonicParsing="false" onAction="#przelicz" text="Refresh" />
   </children>
</AnchorPane>
